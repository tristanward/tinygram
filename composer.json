{
    "name": "tristanward/tinygram",
    "description": "A light-weight Instagram integration.  Pull recent Instagram images into a Laravel website.",
    "keywords": ["instagram","image","feed"],
    "license": "MIT",
    "authors": [
        {
            "name": "Tristan Ward",
            "email": "tristanward1989@gmail.com"
        }
    ],
    "require": {
        "doctrine/dbal": "^2.9",
        "guzzlehttp/guzzle": "^6.3"
    },
    "autoload": {
        "psr-4": {
            "Tristanward\\Tinygram\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tristanward\\Tinygram\\Providers\\TinygramServiceProvider"
            ],
            "aliases": {
                "Tinygram": "Tristanward\\Tinygram\\Facades\\Tinygram"
            }
        }
    }
}
